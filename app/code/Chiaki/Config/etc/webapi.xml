<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route method="POST" url="/V1/chiaki-config/chiakiconfig">
		<service class="Chiaki\Config\Api\ChiakiConfigRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Chiaki_Config::ChiakiConfig_save"/>
		</resources>
	</route>
	<route method="GET" url="/V1/chiaki-config/chiakiconfig/search">
		<service class="Chiaki\Config\Api\ChiakiConfigRepositoryInterface" method="getList"/>
		<resources>
			<resource ref="Chiaki_Config::ChiakiConfig_view"/>
		</resources>
	</route>
	<route method="GET" url="/V1/chiaki-config/chiakiconfig/:chiakiconfigId">
		<service class="Chiaki\Config\Api\ChiakiConfigRepositoryInterface" method="get"/>
		<resources>
			<resource ref="Chiaki_Config::ChiakiConfig_view"/>
		</resources>
	</route>
	<route method="PUT" url="/V1/chiaki-config/chiakiconfig/:chiakiconfigId">
		<service class="Chiaki\Config\Api\ChiakiConfigRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Chiaki_Config::ChiakiConfig_update"/>
		</resources>
	</route>
	<route method="DELETE" url="/V1/chiaki-config/chiakiconfig/:chiakiconfigId">
		<service class="Chiaki\Config\Api\ChiakiConfigRepositoryInterface" method="deleteById"/>
		<resources>
			<resource ref="Chiaki_Config::ChiakiConfig_delete"/>
		</resources>
	</route>
</routes>
