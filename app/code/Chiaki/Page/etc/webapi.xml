<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route method="POST" url="/V1/chiaki-page/chiakipage">
		<service class="Chiaki\Page\Api\ChiakiPageRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Chiaki_Page::ChiakiPage_save"/>
		</resources>
	</route>
	<route method="GET" url="/V1/chiaki-page/chiakipage/search">
		<service class="Chiaki\Page\Api\ChiakiPageRepositoryInterface" method="getList"/>
		<resources>
			<resource ref="Chiaki_Page::ChiakiPage_view"/>
		</resources>
	</route>
	<route method="GET" url="/V1/chiaki-page/chiakipage/:chiakipageId">
		<service class="Chiaki\Page\Api\ChiakiPageRepositoryInterface" method="get"/>
		<resources>
			<resource ref="Chiaki_Page::ChiakiPage_view"/>
		</resources>
	</route>
	<route method="PUT" url="/V1/chiaki-page/chiakipage/:chiakipageId">
		<service class="Chiaki\Page\Api\ChiakiPageRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Chiaki_Page::ChiakiPage_update"/>
		</resources>
	</route>
	<route method="DELETE" url="/V1/chiaki-page/chiakipage/:chiakipageId">
		<service class="Chiaki\Page\Api\ChiakiPageRepositoryInterface" method="deleteById"/>
		<resources>
			<resource ref="Chiaki_Page::ChiakiPage_delete"/>
		</resources>
	</route>
</routes>
